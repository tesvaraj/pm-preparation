# API Keys
# Get your Anthropic API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Get your OpenAI API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Security
# Generate a secure random string for JWT token signing
# You can use: python -c "import secrets; print(secrets.token_urlsafe(32))"
SECRET_KEY=your_secret_key_for_jwt_here

# Database
# SQLite database file location (relative to backend directory)
DATABASE_URL=sqlite:///./pm_practice.db
